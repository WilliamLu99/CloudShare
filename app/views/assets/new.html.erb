<h1>New Asset</h1>



<%= form_for(@asset, multipart: true) do |f| %>
  <div class="field">
    <%= f.label :asset %>
    <%= f.file_field :asset %>
  </div>
  <div class="actions">
    <%= f.submit 'Submit' %>
    <%= link_to 'Link?', '#', class: 'button' %>
  </div>
<% end %>


<%= link_to 'Back', assets_path %>
